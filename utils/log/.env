# 设置缓存大小(如果设置不正确或者为0,默认为10000)
LOG_CACHE_LEN=1000

# 设置log输出级别 "trace" "info" "warning" "error" "fatal" 五中级别
LOG_LEVEL=trace

# log输出引擎 "console" "file" 两种引擎
LOG_ENGINE=console
# log输出文件
LOG_FILE_NAME=log/spider.log
# log文件分割形式 "daily" "size"两种
LOG_FILE_SPILT=size
# log文件大小设置,超过以后自动分割
LOG_FILE_SIZE=30

# 设置是否输出行号
LOG_FUNC_CALL=false

